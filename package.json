{
  "name": "kindle-api",
  "license": "MIT",
  "version": "0.1.1",
  "type": "module",
  "types": "./dist/kindle.d.ts",
  "main": "./dist/kindle.js",
  "exports": {
    ".": {
      "import": "./dist/kindle.js"
    }
  },
  "scripts": {
    "dev": "yarn build:dev",
    "build": "esbuild src/*.ts --platform=node --format=esm --outdir=dist --target=es2021",
    "build:dev": "yarn build --watch",
    "build:production": "tsc && yarn build",
    "test": "vitest --threads=false"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.17",
    "typescript": "^4.9.3",
    "vitest": "^0.27.1"
  },
  "dependencies": {
    "cycletls": "^1.0.21"
  }
}
